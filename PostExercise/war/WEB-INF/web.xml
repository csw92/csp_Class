<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>PostExercise</servlet-name>
		<servlet-class>kr.ac.shinhan.csp.PostExerciseServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PostExercise</servlet-name>
		<url-pattern>/postexercise</url-pattern>
	</servlet-mapping>
		
	<servlet>
		<servlet-name>RetrieveTeamMember</servlet-name>
		<servlet-class>kr.ac.shinhan.csp.RetrieveTeamMemberServlet</servlet-class>
	</servlet>	
	<servlet-mapping>
		<servlet-name>RetrieveTeamMember</servlet-name>
		<url-pattern>/retrieveTeamMember</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ReadTeamMemeber</servlet-name>
		<servlet-class>kr.ac.shinhan.csp.ReadTeamMemberServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ReadTeamMemeber</servlet-name>
		<url-pattern>/readTeamMemeber</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>DeleteMember</servlet-name>
		<servlet-class>kr.ac.shinhan.csp.DeleteMemberServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteMember</servlet-name>
		<url-pattern>/deleteMember</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>UpdateMember</servlet-name>
		<servlet-class>kr.ac.shinhan.csp.UpdateMemberServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UpdateMember</servlet-name>
		<url-pattern>/updateMember</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>SignUp</servlet-name>
		<servlet-class>kr.ac.shinhan.csp.SignUpServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SignUp</servlet-name>
		<url-pattern>/signup</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>kr.ac.shinhan.csp.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Logout</servlet-name>
		<servlet-class>kr.ac.shinhan.csp.LogoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Entry</servlet-name>
		<servlet-class>kr.ac.shinhan.csp.EntryServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Entry</servlet-name>
		<url-pattern>/entry</url-pattern>
	</servlet-mapping>
	
	
	<welcome-file-list>
		<welcome-file>entry</welcome-file>
	</welcome-file-list>
	
	
	
</web-app>
